class BubbleSort
{

static void bubblesort(int a[])
{
int len=a.length;
for(int i=0;i<len;i++)
 for(int j=0;j<len-i-1;j++)
  {
    if(a[j]>a[j+1])
   { int temp=a[j];
     a[j]=a[j+1];
     a[j+1]=temp;
   }
  }

}

static void SortArray(int a[])
{
for(int i=0;i<a.length;i++)
{
 System.out.println(a[i]);
}
}

public static void main(String args[])
{
int a[]={9,1,4,2,7,3,6};
bubblesort(a);
System.out.println("Sorted array");
SortArray(a);
}
}
