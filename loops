import java.io.*;
import java.util.*;




public class Solution {
    public static void main(String[] args){
        Scanner s1=new Scanner(System.in);
        int n1=s1.nextInt();
        for(int i=1;i<=10;i++)
        {
            int result;
            result=n1*i;
            System.out.println(+n1+" x "+i+" = "+result);
        }
    }
}
